<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bean">
	<class name="Teacher" table="teacher_new">
		<id name="id" column="TeacherId" type="integer">
			<generator class="native"/>
		</id>
		<property name="number" column="TeacherNumber" type="string"></property>
		<property name="name" column="TeacherName" type="string"></property>
		<property name="eId" column="ExamNumber" type="string"></property>
		<property name="isadmin" column="IsAdmin" type="string"></property>
		<property name="status" column="Status" type="string"></property>
		<property name="uploadFileName" column="UploadFileName" type="string"></property>
		<property name="uploadFileDate" column="UploadFileDate" type="string"></property>
	</class>
</hibernate-mapping>